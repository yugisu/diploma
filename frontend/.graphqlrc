{
  "schema": {
    "http://localhost:3000/graphql": {
      "headers": {
        "Authorization": "graphql-development"
      }
    }
  },
  "extensions": {
    "endpoints": {
      "default": {
        "url": "http://localhost:3000/graphql",
        "headers": {
          "Authorization": "Bearer jwtToken"
        }
      }
    }
  }
}
